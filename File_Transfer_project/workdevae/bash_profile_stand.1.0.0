        alias atrc='$AUTOSYS/bin/autotrack -v -tC |less'
        alias atrj='$AUTOSYS/bin/autotrack -v -tJ -J'
        alias fadm='autorep -j BSHCfadmrepl -l1'
        alias fsj='$AUTOSYS/bin/sendevent -E FORCE_STARTJOB -J'
        alias hrsis='autorep -j BGHCnightlybatchAPP -l1'
        alias jilcode="cd /usr/local/batch/work/AE_related_backups/jil_code"
        alias jilcode="cd /usr/local/batch/work/WA_related_backups/jil_code"
