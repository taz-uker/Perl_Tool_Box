# Set up the terminal:
        if [ "$TERM" = "" ];
        then
                eval ` tset -s -Q -m ':?hp' `
        else
                eval ` tset -s -Q `
        fi
        stty erase "^H" kill "^U" intr "^C" eof "^D"
        stty hupcl ixon ixoff
	    stty columns 132

# Set up the shell environment:
        set +u 
        trap "echo 'logout'" 0
        EDITOR=vi
        set history=500

